{
    "name": "unocss-preset-utopia",
    "version": "0.2.0",
    "description": "UnoCSS preset that integrates Utopia calculations.",
    "homepage": "https://github.com/adameier/unocss-preset-utopia",
    "license": "MIT",
    "author": "Adam Meier <aameier@protonmail.com>",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/adameier/unocss-preset-utopia.git"
    },
    "keywords": [
        "unocss",
        "utopia",
        "atomic-css",
        "atomic-css-engine",
        "css",
        "tailwind",
        "windicss"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs"
        },
        "./*": "./*"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "stub": "unbuild --stub"
    },
    "dependencies": {
        "@unocss/core": "^0.50.6",
        "@unocss/preset-mini": "^0.50.6"
    },
    "devDependencies": {
        "unbuild": "^1.1.2"
    }
}